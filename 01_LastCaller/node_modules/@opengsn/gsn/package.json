  "scripts": {
    "clean-coverage": "shx rm -rf coverage coverage.json",
    "coverage": "yarn generate && ./scripts/run-coverage",
    "coverage-if-changed": "./scripts/run-coverage-if-changed.sh npm run coverage",
    "extract_abi": "./scripts/extract_abi.js",
    "ganache": "ganache-cli --hardfork 'istanbul' --gasLimit 100000000 --defaultBalanceEther 1000 --deterministic",
    "generate": "rm -rf ./build/contracts && npx truffle compile && typechain --target trufflehotfix './build/contracts/**/*.json'",
    "lint": "yarn run lint:js && yarn run lint:sol",
    "lint:fix": "yarn run lint:js:fix",
    "lint:js": "eslint --ext .js,.ts --cache -f unix --ignore-path .gitignore src test scripts helpers",
    "lint:js:fix": "eslint --ext .js,.ts --cache  -f unix --ignore-path .gitignore src test scripts helpers --fix",
    "lint:sol": "solhint -f unix \"contracts/**/*.sol\" --max-warnings 0",
    "postinstall": "patch-package",
    "prepare": "./scripts/prepublish",
    "test": "yarn run lint && yarn extract_abi && yarn generate && yarn tsc && npm run test-js",
    "test-js": "run-with-testrpc --hardfork 'istanbul' --port 8544 --gasLimit 10000000 --defaultBalanceEther 1000 --deterministic 'npx truffle --network npmtest test'",
    "trace": "MODE=trace truffle test",
    "truffle-compile": "truffle compile",
    "truffle-test": "yarn tsc && npx truffle test",
    "tsc": "tsc --noEmit",
    "web": "./restart-relay.sh web",
    "webtools": "webpack-cli --config ./webtools.webpack.js && http-server webtools/ -o",
    "webtools-pack": "webpack-cli --config ./webtools.webpack.js"
  },
